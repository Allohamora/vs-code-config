{
  "window.zoomLevel": 0,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "emmet.showExpandedAbbreviation": "never",
  "files.associations": {
    "*.js": "javascript",
    "*.jsx": "javascriptreact",
    "*.py": "python"
  },
  "terminal.integrated.shell.windows": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "json.schemas": [],
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "fastFolderStructure.structures": [
    {
      "name": "hook-component",
      "structure": [
        {
          "fileName": "<FFSName>.js",
          "template": "component-with-hook"
        },
        {
          "fileName": "index.js",
          "template": "index"
        },
        {
          "fileName": "use<FFSName>.js",
          "template": "hook"
        },
        {
          "fileName": "service.js"
        }
      ]
    },
    {
      "name": "hook-component.ts",
      "structure": [
        {
          "fileName": "<FFSName>.tsx",
          "template": "component-with-hook.ts"
        },
        {
          "fileName": "index.ts",
          "template": "index"
        },
        {
          "fileName": "use<FFSName>.ts",
          "template": "hook"
        },
        {
          "fileName": "service.ts"
        }
      ]
    },
    {
      "name": "base-component",
      "structure": [
        {
          "fileName": "<FFSName>.js",
          "template": "component"
        },
        {
          "fileName": "index.js",
          "template": "index"
        }
      ]
    },
    {
      "name": "base-component.ts",
      "structure": [
        {
          "fileName": "<FFSName>.tsx",
          "template": "component.ts"
        },
        {
          "fileName": "index.ts",
          "template": "index"
        }
      ]
    }
  ],
  "fastFolderStructure.fileTemplates": {
    "component": [
      "import React from 'react';",
      "",
      "const <FFSName> = () => {",
      "  return (",
      "    <div><FFSName></div>",
      "  );",
      "};",
      "",
      "export default <FFSName>;",
      ""
    ],
    "component.ts": [
      "import React from 'react';",
      "",
      "const <FFSName>: React.FC = () => {",
      "",
      "  return (",
      "    <div><FFSName></div>",
      "  );",
      "};",
      "",
      "export default <FFSName>;",
      ""
    ],
    "component-with-hook": [
      "import React from 'react';",
      "import { use<FFSName> } from './use<FFSName>';",
      "",
      "const <FFSName> = () => {",
      "  const {} = use<FFSName>();",
      "",
      "  return (",
      "    <div><FFSName></div>",
      "  );",
      "};",
      "",
      "export default <FFSName>;",
      ""
    ],
    "component-with-hook.ts": [
      "import React from 'react';",
      "import { use<FFSName> } from './use<FFSName>';",
      "",
      "const <FFSName>: React.FC = () => {",
      "  const {} = use<FFSName>();",
      "",
      "  return (",
      "    <div><FFSName></div>",
      "  );",
      "};",
      "",
      "export default <FFSName>;",
      ""
    ],
    "hook": [
      "export const use<FFSName> = () => {",
      "  return {};",
      "};"
    ],
    "index": [
      "export { default } from './<FFSName>';"
    ]
  }
}